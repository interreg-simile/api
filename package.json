  "bugs": {
    "url": "https://github.com/epessina/SIMILE-App/issues"
  },
  "homepage": "https://github.com/epessina/SIMILE-App#readme",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.7.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "helmet": "^3.21.2",
    "i18next": "^19.1.0",
    "i18next-node-fs-backend": "^2.1.3",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
